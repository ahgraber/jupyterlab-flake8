{
  "name": "jupyterlab-flake8",
  "version": "0.3.2",
  "description": "jupyterlab extension to lint python code in the notebook and text editor",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/mlshapiro/jupyterlab-flake8",
  "bugs": {
    "url": "https://github.com/mlshapiro/jupyterlab-flake8/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Marc Shapiro",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mlshapiro/jupyterlab-flake8.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf lib",
    "watch": "tsc -w",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.1",
    "@jupyterlab/apputils": "^1.2.1",
    "@jupyterlab/cells": "^1.2.1",
    "@jupyterlab/codemirror": "^1.2.1",
    "@jupyterlab/coreutils": "^3.2.0",
    "@jupyterlab/fileeditor": "^1.2.1",
    "@jupyterlab/mainmenu": "^1.2.1",
    "@jupyterlab/notebook": "^1.2.2",
    "@jupyterlab/terminal": "^1.2.1",
    "@types/codemirror": "0.0.80",
    "codemirror": "^5.49.2"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "shx": "^0.3.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.6.2"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
